#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/@antfu+eslint-config@5.1.0_@unocss+eslint-plugin@66.4.0_eslint@9.32.0_jiti@2.4.2__types_0f5ca257b1d351577a4933e47e8c0879/node_modules/@antfu/eslint-config/bin/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/@antfu+eslint-config@5.1.0_@unocss+eslint-plugin@66.4.0_eslint@9.32.0_jiti@2.4.2__types_0f5ca257b1d351577a4933e47e8c0879/node_modules/@antfu/eslint-config/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/@antfu+eslint-config@5.1.0_@unocss+eslint-plugin@66.4.0_eslint@9.32.0_jiti@2.4.2__types_0f5ca257b1d351577a4933e47e8c0879/node_modules/@antfu/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/@antfu+eslint-config@5.1.0_@unocss+eslint-plugin@66.4.0_eslint@9.32.0_jiti@2.4.2__types_0f5ca257b1d351577a4933e47e8c0879/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/@antfu+eslint-config@5.1.0_@unocss+eslint-plugin@66.4.0_eslint@9.32.0_jiti@2.4.2__types_0f5ca257b1d351577a4933e47e8c0879/node_modules/@antfu/eslint-config/bin/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/@antfu+eslint-config@5.1.0_@unocss+eslint-plugin@66.4.0_eslint@9.32.0_jiti@2.4.2__types_0f5ca257b1d351577a4933e47e8c0879/node_modules/@antfu/eslint-config/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/@antfu+eslint-config@5.1.0_@unocss+eslint-plugin@66.4.0_eslint@9.32.0_jiti@2.4.2__types_0f5ca257b1d351577a4933e47e8c0879/node_modules/@antfu/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/@antfu+eslint-config@5.1.0_@unocss+eslint-plugin@66.4.0_eslint@9.32.0_jiti@2.4.2__types_0f5ca257b1d351577a4933e47e8c0879/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
fi
