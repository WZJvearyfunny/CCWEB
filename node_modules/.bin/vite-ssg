#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/vite-ssg@28.0.0_prettier@3.5.3_unhead@2.0.13_vite@7.0.6_@types+node@24.1.0_jiti@2.4.2_s_2a1c581efee6ea06aba12d170c0e8884/node_modules/vite-ssg/bin/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/vite-ssg@28.0.0_prettier@3.5.3_unhead@2.0.13_vite@7.0.6_@types+node@24.1.0_jiti@2.4.2_s_2a1c581efee6ea06aba12d170c0e8884/node_modules/vite-ssg/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/vite-ssg@28.0.0_prettier@3.5.3_unhead@2.0.13_vite@7.0.6_@types+node@24.1.0_jiti@2.4.2_s_2a1c581efee6ea06aba12d170c0e8884/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/vite-ssg@28.0.0_prettier@3.5.3_unhead@2.0.13_vite@7.0.6_@types+node@24.1.0_jiti@2.4.2_s_2a1c581efee6ea06aba12d170c0e8884/node_modules/vite-ssg/bin/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/vite-ssg@28.0.0_prettier@3.5.3_unhead@2.0.13_vite@7.0.6_@types+node@24.1.0_jiti@2.4.2_s_2a1c581efee6ea06aba12d170c0e8884/node_modules/vite-ssg/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/vite-ssg@28.0.0_prettier@3.5.3_unhead@2.0.13_vite@7.0.6_@types+node@24.1.0_jiti@2.4.2_s_2a1c581efee6ea06aba12d170c0e8884/node_modules:/Users/pipedream/Desktop/workbase/CCWeb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-ssg/bin/vite-ssg.js" "$@"
else
  exec node  "$basedir/../vite-ssg/bin/vite-ssg.js" "$@"
fi
